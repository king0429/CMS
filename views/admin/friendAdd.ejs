<% include header.ejs%>
  <!-- 输入框组 --> 
  <div class="container" style="margin-bottom: 50px;">
    <h3 style="text-align: center;margin-bottom: 30px">添加链接</h3> 
    <div class="row">
      <div class="col-md-3"></div>
      <div class="col-md-6">
        <form action="/admin/friendInsert" method="post">
          <div class="form-group">
            <label>网站名称</label>
            <input type="text" class="form-control"  name="name">
          </div>  
          <div class="form-group">
            <label for="user_name">关键字</label>
            <input type="text" class="form-control" id="user_name" name="keyword">
          </div>
          <div class="form-group">
            <label for="cat">网站地址</label>
            <input type="text" class="form-control" id="cat" name="url">
          </div>
          <div class="form-group">
            <label for="cat">联系方式</label>
            <input type="number" class="form-control" id="cat" name="connecter">
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
        </form>
      </div>
      <div class="col-md-3"></div>
    </div>
  </div>
  <!-- 输入框组 END -->
<% include footer.ejs%>

